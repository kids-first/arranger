{
  "name": "@kfarranger/server",
  "version": "1.19.0",
  "description": "GraphQL Server",
  "main": "dist/index.js",
  "bin": {
    "arranger-server": "./index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "DEPS=`node getDependencies.js` && cd .. && nodemon $DEPS server/index.js",
    "run-prod": "node index.js",
    "prepare": "npm run snyk-protect && rm -rf dist && mkdir dist && babel src --ignore */**/__tests__ --out-dir dist",
    "test": "jest",
    "watch": "rm -rf dist && mkdir dist && babel src --watch --out-dir dist",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kids-first/arranger.git"
  },
  "author": "OICR",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/kids-first/arranger/issues"
  },
  "homepage": "https://github.com/kids-first/arranger#readme",
  "dependencies": {
    "@kfarranger/admin": "1.17.0",
    "@kfarranger/mapping-utils": "1.18.0",
    "@kfarranger/middleware": "1.9.0",
    "@kfarranger/schema": "1.19.0",
    "apollo-server-core": "1.3.2",
    "apollo-server-express": "^1.3.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.1",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.0",
    "elasticsearch": "^14.0.0",
    "express": "^4.16.2",
    "graphql": "^14.0.2",
    "jsonpath": "^1.0.0",
    "node-fetch": "^2.0.0",
    "tar-stream": "^1.5.5",
    "through2": "^2.0.3",
    "url-join": "^4.0.0",
    "uuid": "^3.2.1",
    "snyk": "^1.622.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/register": "7.8.6",
    "jest": "25.1.0",
    "nodemon": "^1.18.4"
  },
  "jest": {
    "testURL": "http://localhost/"
